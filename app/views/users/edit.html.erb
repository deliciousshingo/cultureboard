<%= render "layouts/shared/header" %>
<div class="contents row">
  <div class="container">
    <h2>プロフィール編集</h2>
    <%= form_with model: @user, url: user_path(@user.id), id: 'edit_user', class: 'new_user', local: true do |f| %>
      <%= render 'layouts/shared/error_messages', model: f.object %>
      <%= render partial: 'users/form', locals: { f: f } %>

      <div class="action">
        <%= f.submit "編集" %>
      </div>
    <% end %>
  </div>
</div>
 <%= render "layouts/shared/footer" %>